{
	"name": "lyteui",
	"version": "0.0.5",
	"description": "React UI Library for Lyte Ventures",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"test": "jest",
		"test-watch": "jest --watch",
		"delete-dist": "rimraf dist",
		"build-dist-css": "sass --no-source-map assets/scss/index.scss:dist/css/index.css",
		"build-dist-com": "rollup -c",
		"build": "run-s delete-dist build-dist-*",
		"build-docs": "build-storybook -o docs",
		"docs": "start-storybook -p 6006",
		"generate-toc": "markdown-toc -i --bullets=- README.md",
		"check-publish": "npm pack && rimraf ./*.tgz"
	},
	"author": "Chew Han Xiang <hanxiang.chew@lyteventures.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/chewhx/lyteui.git"
	},
	"license": "MIT",
	"keywords": [
		"react",
		"ui",
		"ui-lib",
		"bootstrap",
		"react-bootstrap",
		"frontend",
		"design"
	],
	"dependencies": {
